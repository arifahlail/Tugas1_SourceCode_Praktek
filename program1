public class Asgdll {
    public static void main(String[] args) {
        // Contoh penggunaan tipe data float
        float f = 20.0f;
        double fll;
	fll= 10.0f;
        System.out.println("Contoh penggunaan tipe data float:");
        System.out.println("f : " +f+ "\nfll: " +fll);
        
    }
}
